{
    "Serilog": {
      "Using": ["Serilog.Sinks.Console", "Serilog.Sinks.Graylog"],
      "MinimumLevel": {
        "Default": "Information",
        "Override": {
          "Microsoft": "Information",
          "System": "Information"
        }
      },
      "WriteTo": [
        { "Name": "Console" },
        {
          "Name": "Graylog",
          "Args": {
            "hostnameOrAddress": "127.0.0.1",
            "port": 12201,
            "transportType": "Udp"
          }
        }
      ],
      "Enrich": ["FromLogContext"],
      "Properties": {
        "Application": "DateApp Asp.NetCore"
      }
    }
  }